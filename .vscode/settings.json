{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  },
  "prettier.configPath": ".prettierrc.js",
  "prettier.enableDebugLogs": true,
  "prettier.useEditorConfig": false,
  "search.exclude": {
    "**/dist": true,
    "**/node_modules": true
  },
  "files.watcherExclude": {
    "**/dist": true,
    "**/node_modules": true
  }
}
